        "/api/createRutina": {
            "post": {
                "tags": [
                    "Rutinas"
                ],
                "summary": "Crear una rutina",
                "operationId": "08a4c4b858feddeda0e7342c5f1a475c",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "id_inscripcion",
                                    "fecha_rutina",
                                    "rondas",
                                    "tiempo",
                                    "peso",
                                    "halterofilia",
                                    "ejercicios"
                                ],
                                "properties": {
                                    "id_inscripcion": {
                                        "type": "integer",
                                        "example": "2"
                                    },
                                    "fecha_rutina": {
                                        "type": "string",
                                        "format": "date",
                                        "example": "2024-10-10"
                                    },
                                    "rondas": {
                                        "type": "integer",
                                        "example": 5
                                    },
                                    "tiempo": {
                                        "type": "integer",
                                        "example": 10
                                    },
                                    "peso": {
                                        "type": "number",
                                        "format": "float",
                                        "example": 45
                                    },
                                    "halterofilia": {
                                        "type": "integer",
                                        "enum": [
                                            0,
                                            1
